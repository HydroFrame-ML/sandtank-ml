{
  "ui": {
    "time": [1, 1],
    "moduleSelector": {
      "show": false,
      "values": [ "selection", "prediction", "diff" ]
    },
    "diffScaling": {
      "show": false,
      "value": 0.5,
      "min": 0.1,
      "step": 0.1,
      "max": 1
    },
    "useGradient": {
      "show": false,
      "value": true
    },
    "addRemoveAI": {
      "show": false,
      "defaultModels": [ 
        { 
          "model": "RegressionPressure",
          "training":"full",
          "learningRate": "lr4",
          "dropOut":"ndp",
          "epoch":"e1"
        },
        { 
          "model": "RegressionPressure",
          "training":"wet",
          "learningRate": "lr4",
          "dropOut":"ndp",
          "epoch":"e1"
        },
        { 
          "model": "RegressionPressure",
          "training":"dry",
          "learningRate": "lr4",
          "dropOut":"ndp",
          "epoch":"e1"
        }
      ]
    }

  },
  "order": ["model", "training", "learningRate", "dropOut", "epoch"],
  "parameters": {
    "model": {
      "label": "Model",
      "items": [{ "text": "Pressure", "value": "RegressionPressure" }]
    },
    "training": {
      "label": "Learning set",
      "items": [
        { "text": "All", "value": "full" },
        { "text": "Wet", "value": "wet" },
        { "text": "Dry", "value": "dry" }
      ]
    },
    "learningRate": {
      "label": "Learning Rate",
      "items": [
        { "text": "0.0001", "value": "lr4" }
      ]
    },
    "dropOut": {
      "label": "Use drop out",
      "items": [
        { "text": "No", "value": "ndp" }
      ]
    },
    "epoch": {
      "label": "Number of trainings",
      "items": [
        { "value": "e1", "text": "One time" }
      ]
    }
  },
  "uriPattern":
  "${model}://${training}-${learningRate}-${dropOut}-${epoch}.out"
}
