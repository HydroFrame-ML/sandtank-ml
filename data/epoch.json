{
    "ui": {
      "time": [-1, 5],
      "moduleSelector": {
        "show": true,
        "values": ["selection", "prediction", "diff"]
      },
      "diffScaling": {
        "show": true,
        "value": 0.5,
        "min": 0.1,
        "step": 0.1,
        "max": 1
      },
      "useGradient": {
        "show": true,
        "value": true
      }
    },
    "order": ["epoch"],
    "parameters": {
      "epoch": {
        "label": "Number of trainings",
        "items": [
          { "value": "e1", "text": "1" },
          { "value": "e10", "text": "10" },
          { "value": "e50", "text": "50" }
        ]
      }
    },
    "uriPattern":
      "RegressionPressure://models/odd-lr4-ndp-${epoch}.out"
  }