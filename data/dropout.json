{
    "ui": {
      "time": [-1, 5],
      "moduleSelector": {
        "show": true,
        "values": ["selection", "prediction", "diff"]
      },
      "diffScaling": {
        "show": true,
        "value": 0.5,
        "min": 0.1,
        "step": 0.1,
        "max": 1
      },
      "usePressure": {
        "show": true,
        "value": true
      }
    },
    "order": ["model", "training", "learningRate", "dropOut", "epoch"],
    "parameters": {
      "model": {
        "label": "Model",
        "items": [{ "text": "Pressure", "value": "RegressionPressure" }]
      },
      "training": {
        "label": "Training set",
        "items": [
            { "text": "All", "value": "full" }
        ]
      },
      "learningRate": {
        "label": "Learning Rate",
        "items": [
            { "text": "0.0001", "value": "lr4" }
        ]
      },
      "dropOut": {
        "label": "Use drop out",
        "items": [
            { "text": "Yes", "value": "dp" }
        ]
      },
      "epoch": {
        "label": "Number of trainings",
        "items": [
          { "value": "e1", "text": "1" },
          { "value": "e10", "text": "10" },
          { "value": "e31", "text": "31" }
        ]
      }
    },
    "uriPattern":
      "${model}://${training}-${learningRate}-${dropOut}-${epoch}.out"
  }
