{
  "ui": {
    "time": [-1, 5],
    "moduleSelector": {
      "show": true,
      "values": ["selection", "prediction", "diff", "hist", "error", "stats"]
    },
    "diffScaling": {
      "show": true,
      "value": 0.5,
      "min": 0.1,
      "step": 0.1,
      "max": 1
    },
    "useGradient": {
      "show": true,
      "value": false 
    },
    "addRemoveAI": {
      "show": true,
      "defaultModels": []
    },
    "useHistGlobalMax": {
      "show": true,
      "value": true
    },
    "usePermeabilityLabels": {
      "show": true,
      "values": {
        "0": "A River",
        "1": "Sand",
        "0.6": "Compacted Sand",
        "0.05": "Dense Clay"
      }
    },
    "useDiffLabels": {
      "show": true,
      "values": {
        "0": "Accurate",
        "1": "Error"
      }
    },
    "useWaterLabels": {
      "show": true
    },
    "useSkipInitial" : {
      "show": true,
      "value": true
    }
  },
  "order": ["model", "training", "learningRate", "dropOut", "epoch"],
  "parameters": {
    "model": {
      "label": "Model",
      "items": [{ "text": "Pressure", "value": "RegressionPressure" }]
    },
    "training": {
      "label": "Learning set",
      "items": [
        { "text": "All", "value": "full" },
        { "text": "Wet", "value": "wet" },
        { "text": "Dry", "value": "dry" }
      ]
    },
    "learningRate": {
      "label": "Learning Rate",
      "items": [
        { "text": "0.0001", "value": "lr4" }
      ]
    },
    "dropOut": {
      "label": "Use drop out",
      "items": [
        { "text": "No", "value": "ndp" }
      ]
    },
    "epoch": {
      "label": "Number of trainings",
      "items": [
        { "value": "e1", "text": "One time" }
      ]
    }
  },
  "uriPattern":
  "${model}://models/${training}-${learningRate}-${dropOut}-${epoch}.out"
  }
