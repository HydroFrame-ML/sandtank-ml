{
  "configuration": {
    "host": "localhost",
    "port": 9000,
    "endpoint": "paraview",
    "proxy_file": "/opt/launcher/proxy-mapping.txt",
    "sessionURL": "ws://${host}:${port}/ws/proxy?sessionId=${id}&path=ws",
    "timeout": 5,
    "log_dir": "/home/ubuntu/",
    "fields": [
      "file",
      "host",
      "port",
      "updir"
    ]
  },
  "resources": [
    {
      "host": "localhost",
      "port_range": [
        9001,
        9003
      ]
    }
  ],
  "properties": {
    "python_exec": "/opt/paraview/bin/pvpython"
  },
  "apps": {
    "visualizer": {
      "cmd": [
        "${python_exec}",
        "-dr",
        "${visualizer_path}/server/server.py",
        "--port",
        "${port}",
        "--data",
        "${dataDir}"
      ],
      "ready_line": "Starting factory"
    }
  }
}
