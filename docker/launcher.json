{
  "configuration": {
    "host": "localhost",
    "port": 8080,
    "endpoint": "paraview",
    "content": "/.../www",
    "proxy_file": "<MAPPING-FILE-DIR>/proxy.txt",
    "sessionURL": "ws://${host}:${port}/ws",
    "timeout": 5,
    "log_dir": "/home/ubuntu/",
    "upload_dir": "/home/ubuntu/",
    "fields": [
      "file",
      "host",
      "port",
      "updir"
    ]
  },
  "resources": [
    {
      "host": "localhost",
      "port_range": [
        9001,
        9003
      ]
    }
  ],
  "properties": {
    "build_dir": "/.../build",
    "python_exec": "/.../build/bin/vtkpython",
    "WWW": "/.../build/www",
    "source_dir": "/.../src"
  },
  "apps": {
    "visualizer": {
      "cmd": [
        "${python_exec}",
        "-dr",
        "${visualizer_path}/server/server.py",
        "--port",
        "${port}",
        "--data",
        "${dataDir}"
      ],
      "ready_line": "Starting factory"
    }
  }
}
